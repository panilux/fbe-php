{
    "name": "panilux/fbe",
    "description": "Fast Binary Encoding runtime library for PHP",
    "type": "library",
    "license": "MIT",
    "keywords": ["serialization", "binary", "encoding", "fbe"],
    "authors": [
        {
            "name": "Panilux Team"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FBE\\": "src/FBE/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FBE\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/fbec"]
}

