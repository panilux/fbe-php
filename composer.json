{
    "name": "panilux/fbe-php",
    "description": "Fast Binary Encoding (FBE) for PHP - High-performance binary serialization library with 100% FBE specification alignment",
    "type": "library",
    "license": "MIT",
    "keywords": ["fbe", "fast-binary-encoding", "serialization", "binary", "encoding", "protocol", "panilux"],
    "authors": [
        {
            "name": "Panilux Team",
            "email": "dev@panilux.com",
            "homepage": "https://panilux.com"
        }
    ],
    "homepage": "https://github.com/panilux/fbe-php",
    "require": {
        "php": ">=8.4",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "FBE\\": "src/FBE/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FBE\\Tests\\": "tests/"
        }
    },
    "bin": ["bin/fbec"],
    "scripts": {
        "test": "php test_types.php && php test_collections.php && php test_inheritance.php"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

